PKGDES="Utilities for displaying information about running process"
PKGDEP="ncurses"
PKGSEC=utils

AUTOTOOLS_AFTER="--enable-nls \
                 --disable-selinux \
                 --disable-apparmor \
                 --enable-harden-flags \
                 --enable-ipv6 \
                 --disable-rpath \
                 --enable-largefile"

PKGESS=1
