PKGNAME=edb-debugger
PKGSEC=devel
PKGDEP="qt-5 desktop-file-utils capstone libproxy"
BUILDDEP="boost"
PKGDES="EDB (Evan's Debugger) is a QT based binary mode debugger with the goal of having usability on par with OllyDbg."

AB_FLAGS_SPECS=0
AB_FLAGS_PIC=1

PKGBREAK="edb<=1.2.0"
PKGREP="edb<=1.2.0"

FAIL_ARCH="!(amd64)"
