+static void phytmac_get_hw_stats(struct phytmac *pdata)
+{
+	u32 stats[45];
+	int i, j;
+	u64 val;
+	u64 *p = &pdata->stats.tx_octets;
+
+	for (i = 0 ; i < 45; i++)
+		stats[i] = PHYTMAC_READ(pdata, PHYTMAC_OCTTX + i * 4);
+
+	for (i = 0, j = 0; i < 45; i++) {
+		if (i == 0 || i == 20) {
+			val = (u64)stats[i + 1] << 32 | stats[i];
+			*p += val;
+			pdata->ethtool_stats[j] = *p;
+			++j;
+			++p;
+		} else {
+			if (i != 1 && i != 21) {
+				val = stats[i];
+				*p += val;
+				pdata->ethtool_stats[j] = *p;
+				++j;
+				++p;
+			}
+		}
+	}
+}
