+inline struct phytmac_dma_desc *phytmac_get_rx_desc(struct phytmac_queue *queue,
+					     unsigned int index)
+{
+	return &queue->rx_ring[index & (queue->pdata->rx_ring_size - 1)];
+}
+
+struct sk_buff *phytmac_get_rx_skb(struct phytmac_queue *queue,
