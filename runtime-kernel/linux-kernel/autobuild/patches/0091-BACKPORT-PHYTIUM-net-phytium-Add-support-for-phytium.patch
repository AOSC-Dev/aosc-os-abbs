+static void phytmac_mdio_idle(struct phytmac *pdata)
+{
+	u32 val;
+
+	/* wait for end of transfer */
+	val = PHYTMAC_READ(pdata, PHYTMAC_NSTATUS);
+	while (!(val & PHYTMAC_BIT(MDIO_IDLE))) {
+		cpu_relax();
+		val = PHYTMAC_READ(pdata, PHYTMAC_NSTATUS);
+	}
+}
