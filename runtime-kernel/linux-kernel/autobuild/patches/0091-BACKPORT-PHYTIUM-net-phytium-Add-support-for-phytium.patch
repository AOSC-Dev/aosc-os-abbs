+		      | PHYTMAC_BITS(OPS, PHYTMAC_OPS_C45_WRITE)
+		      | PHYTMAC_BITS(PHY_ADDR, mii_id)
+		      | PHYTMAC_BITS(REG_ADDR, devad & 0x1F)
