+		      | PHYTMAC_BITS(OPS, PHYTMAC_OPS_C45_READ)
+		      | PHYTMAC_BITS(PHY_ADDR, mii_id)
+		      | PHYTMAC_BITS(REG_ADDR, devad & 0x1F)
+		      | PHYTMAC_BITS(DATA, regnum & 0xFFFF)
+		      | PHYTMAC_BITS(MUST, 2)));
+
+	phytmac_mdio_idle(pdata);
+	data = PHYTMAC_READ(pdata, PHYTMAC_MDATA) & 0xffff;
