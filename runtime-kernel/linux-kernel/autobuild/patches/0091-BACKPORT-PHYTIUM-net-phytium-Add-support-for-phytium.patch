+static int phytmac_calc_rx_buf_len(struct phytmac *pdata, u32 mtu)
+{
+	unsigned int size = mtu + ETH_HLEN + ETH_FCS_LEN;
+	int rx_buf_len = roundup(size, RX_BUFFER_MULTIPLE);
+
+	netdev_dbg(pdata->ndev, "mtu [%u] rx_buffer_size [%u]\n",
+		   mtu, rx_buf_len);
+
+	return rx_buf_len;
+}
