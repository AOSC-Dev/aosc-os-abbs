
# Listening address
V2RAYA_ADDRESS=0.0.0.0:2017

# Executable v2ray binary path. Auto-detect if put it empty.
V2RAYA_V2RAY_BIN=/usr/bin/v2ray

# v2rayA configuration directory
V2RAYA_CONFIG=/etc/v2raya

