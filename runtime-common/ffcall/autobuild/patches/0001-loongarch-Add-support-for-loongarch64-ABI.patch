+/* Available registers: $r12 ... $r20. */
+
+	.text
+	.align	3
+	.globl	tramp
+	.type	tramp, @function
+tramp:
+	pcaddu12i	$r12, 0
+	/* Now our own address (=tramp) is in $r12. */
+	ld.d		$r13, $r12, 24 /* $LC0-tramp */
+	ld.d		$r14, $r12, 32 /* $LC1-tramp */
+	st.d		$r14, $r13, 0
+	ld.d		$r12, $r12, 40 /* $LC2-tramp */
+	jr		$r12
