+			__le16 pvb;
+
+			if (pstapriv->tim_bitmap&0xff00)
+				pvb = cpu_to_le16(pstapriv->tim_bitmap >> 8);
+			else
+				pvb = tim_bitmap_le;
+
+			*dst_ie++ = le16_to_cpu(pvb);
+
+		}
+		else if (tim_ielen ==5)
+		{
+			memcpy(dst_ie, &tim_bitmap_le, 2);
+			dst_ie+=2;
+		}
+
