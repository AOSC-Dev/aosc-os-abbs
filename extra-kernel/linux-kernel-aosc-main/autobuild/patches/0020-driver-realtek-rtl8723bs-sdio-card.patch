+	if (!(regEAC & BIT28) &&
+	    (((regE94 & 0x03FF0000)>>16) != 0x142) &&
+	    (((regE9C & 0x03FF0000)>>16) != 0x42) &&
+	    (((regE94 & 0x03FF0000)>>16) <0x110) &&
+	    (((regE94 & 0x03FF0000)>>16) >0xf0) &&
+	    (tmp <0xf))
