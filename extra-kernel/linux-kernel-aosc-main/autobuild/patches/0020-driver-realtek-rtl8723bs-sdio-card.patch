+static void
+halbtc8723b2ant_DecBtPwr(
+PBTC_COEXIST		pBtCoexist,
+bool			bForceExec,
+u8 		decBtPwrLvl
+	)
+{
+	BTC_PRINT(BTC_MSG_ALGORITHM, ALGO_TRACE_FW, ("[BTCoex], %s Dec BT power level = %d\n",
+		(bForceExec? "force to":""), decBtPwrLvl));
+	pCoexDm->curBtDecPwrLvl = decBtPwrLvl;
+
