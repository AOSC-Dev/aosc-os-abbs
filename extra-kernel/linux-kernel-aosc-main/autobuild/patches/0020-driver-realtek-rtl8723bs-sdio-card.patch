+static u8 key_char2num(u8 ch);
+static u8 key_char2num(u8 ch)
+{
+    if ((ch>='0') && (ch<='9'))
+        return ch - '0';
+    else if ((ch>='a') && (ch<='f'))
+        return ch - 'a' + 10;
+    else if ((ch>='A') && (ch<='F'))
+        return ch - 'A' + 10;
+    else
+	 return 0xff;
+}
+
+u8 key_2char2num(u8 hch, u8 lch);
+u8 key_2char2num(u8 hch, u8 lch)
+{
+    return ((key_char2num(hch) << 4) | key_char2num(lch));
+}
