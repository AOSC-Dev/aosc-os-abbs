diff -Naur linux-4.3/drivers/staging/rtl8723bs/include/wlan_bssdef.h linux-4.3.patched/drivers/staging/rtl8723bs/include/wlan_bssdef.h
--- linux-4.3/drivers/staging/rtl8723bs/include/wlan_bssdef.h	1970-01-01 08:00:00.000000000 +0800
+++ linux-4.3.patched/drivers/staging/rtl8723bs/include/wlan_bssdef.h	2015-11-12 22:00:56.650390214 +0800
@@ -0,0 +1,278 @@
+/******************************************************************************
+ *
+ * Copyright(c) 2007 - 2011 Realtek Corporation. All rights reserved.
+ *
+ * This program is free software; you can redistribute it and/or modify it
+ * under the terms of version 2 of the GNU General Public License as
+ * published by the Free Software Foundation.
