+	switch (pattrib->qsel)
+	{
+		case 0:
+		case 3:
+			addr = BE_QUEUE_INX;
+			break;
+		case 1:
+		case 2:
+			addr = BK_QUEUE_INX;
+			break;
+		case 4:
+		case 5:
+			addr = VI_QUEUE_INX;
+			break;
+		case 6:
+		case 7:
+			addr = VO_QUEUE_INX;
+			break;
+		case 0x10:
+			addr = BCN_QUEUE_INX;
+			break;
+		case 0x11:/* BC/MC in PS (HIQ) */
+			addr = HIGH_QUEUE_INX;
+			break;
+		case 0x12:
+		default:
+			addr = MGT_QUEUE_INX;
+			break;
+
+	}
+
+	return addr;
+
