+/************************************/
+/* bitwise_xor()                    */
+/* A 128 bit, bitwise exclusive or  */
+/************************************/
+static void bitwise_xor(u8 *ina, u8 *inb, u8 *out)
+{
+    sint i;
+
+    for (i = 0; i<16; i++)
+    {
+        out[i] = ina[i] ^ inb[i];
+    }
+}
