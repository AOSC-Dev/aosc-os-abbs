+
+	PHY_SetBBReg(pDM_Odm->Adapter, ODM_REG(BB_ATC, pDM_Odm), ODM_BIT(BB_ATC, pDM_Odm), ATCStatus);
+	pCfoTrack->bATCStatus = ATCStatus;
+}
