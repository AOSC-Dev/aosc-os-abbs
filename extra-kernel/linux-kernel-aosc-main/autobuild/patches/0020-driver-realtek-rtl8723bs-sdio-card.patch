+/*  */
+/*  Endianness before calling this API */
+/*  */
+static void
+ODM_PhyStatusQuery_92CSeries(
+	PDM_ODM_T					pDM_Odm,
+	PODM_PHY_INFO_T				pPhyInfo,
+	u8 *					pPhyStatus,
+	PODM_PACKET_INFO_T			pPktinfo
+	)
+{
+
+	odm_RxPhyStatus92CSeries_Parsing(
+							pDM_Odm,
+							pPhyInfo,
+							pPhyStatus,
+							pPktinfo);
+
