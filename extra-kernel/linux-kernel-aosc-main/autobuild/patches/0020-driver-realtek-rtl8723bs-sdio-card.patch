+static void
+Hal_EfuseParseMACAddr_8723BS(
+struct adapter *	padapter,
+u8*		hwinfo,
+bool			AutoLoadFail
+	)
+{
+	u16 		i;
+	u8 	sMacAddr[6] = {0x00, 0xE0, 0x4C, 0xb7, 0x23, 0x00};
+	struct eeprom_priv *pEEPROM = GET_EEPROM_EFUSE_PRIV(padapter);
+
+	if (AutoLoadFail)
+	{
+/* 		sMacAddr[5] = (u8)GetRandomNumber(1, 254); */
