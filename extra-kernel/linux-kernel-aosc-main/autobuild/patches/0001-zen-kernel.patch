diff --git a/ipc/kdbus/names.c b/ipc/kdbus/names.c
new file mode 100644
index 0000000..bf44ca3
--- /dev/null
+++ b/ipc/kdbus/names.c
@@ -0,0 +1,854 @@
+/*
