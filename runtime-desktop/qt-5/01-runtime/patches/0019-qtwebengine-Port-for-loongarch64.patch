+void Assembler::GetCode(Isolate* isolate, CodeDesc* desc,
+                        SafepointTableBuilder* safepoint_table_builder,
+                        int handler_table_offset) {
+  // EmitForbiddenSlotInstruction(); // TODO why?
+
+  int code_comments_size = WriteCodeComments();
