+    case kLa64Ext:
+    case kLa64Ins:
+    case kLa64Dext:
+    case kLa64Dins:
+    case kLa64Dshl:
+    case kLa64Dshr:
+    case kLa64Dsar:
+    case kLa64Ror:
+    case kLa64Dror:
+      return 1;
+    case kLa64Tst:
