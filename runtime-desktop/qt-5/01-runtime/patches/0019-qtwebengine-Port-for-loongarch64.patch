+      if (is_operand_register) {
+        return latency + 2;
+      } else {
+        return latency + 1;
+      }
+    }
+    case kLa64Clz:
+    case kLa64Dclz:
+      return DclzLatency();
+    case kLa64Ctz:
+      return CtzLatency();
+    case kLa64Dctz:
+      return DctzLatency();
+    case kLa64Popcnt:
